inputDir="testcases/input"
outputDir="testcases/myoutput"
correctOutputDir="testcases/output"

if [ ! -e "$outputDir" ]; then
	mkdir "$outputDir"
fi

for i in `seq 1 21`
do
	if [ -e "$inputDir/test$i.micro" ]; then
		./Micro "$inputDir/test$i.micro" > "$outputDir/test$i.out"  2>&1
		echo $i
		diff "$outputDir/test$i.out" "$correctOutputDir/test$i.out"
	fi
done
